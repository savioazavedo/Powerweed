@model IEnumerable<PWBackend.Fleet>

@{
    ViewBag.Title = "Index";
}
<img src="~/Images/newlogo.png" alt="Powerweed logo" style="width:304px;height:110px;">
<h2>
    @Html.ActionLink("Create New", "Create")
</h2>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.FLEETNumber)*@
            <h4>Fleet Number</h4>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.FLEETName)*@
            <h4>Fleet Name</h4>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.FLEETDescription)*@
            <h4>Fleet Description</h4>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.FLEETRego)*@
            <h4>Fleet REGO</h4>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.FLEETNumber)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FLEETName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FLEETDescription)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FLEETRego)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.FLEETID }) |
            @Html.ActionLink("Details", "Details", new { id=item.FLEETID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.FLEETID })
        </td>
    </tr>
}

</table>
