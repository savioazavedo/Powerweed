@model IEnumerable<PWBackend.Admin>

@{
    ViewBag.Title = "Index";
}

<img src="~/Images/newlogo.png" alt="Powerweed logo" style="width:304px;height:110px;" />
<h2>
    @Html.ActionLink("Create New", "Create")
</h2>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.AdminNAME)*@
            <h4>Admin Name</h4>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.AdminPASSWORD)*@
            <h4>Password</h4>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.AdminPIN)*@
            <h4>Pin Number</h4>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.AdminNAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminPASSWORD)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.AdminPIN)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.AdminID }) |
            @Html.ActionLink("Details", "Details", new { id=item.AdminID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.AdminID })
        </td>
    </tr>
}

</table>
