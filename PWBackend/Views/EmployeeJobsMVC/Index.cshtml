@model IEnumerable<PWBackend.EmployeeJob>

@{
    ViewBag.Title = "Index";
}

<h4>Employees Jobs</h4>

<p>
    <h2>@Html.ActionLink("Create New", "Create")</h2>
</p>
<table class="table">
    <tr>
        <th>
            @*@Html.DisplayNameFor(model => model.EmpNAME)*@
            <h4>Employee Name</h4>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.JobsAssigned.AssignJOBNUM)*@
            <h4>Job Number</h4>
        </th>
        <th>
            @*@Html.DisplayNameFor(model => model.JobsAssigned.AssignJOBNUM)*@
            <h4>Assign ID</h4>
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EmpNAME)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JobsAssigned.AssignJOBNUM)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.JobsAssigned.AssignID)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.EmployeeJOBSID }) |
            @Html.ActionLink("Details", "Details", new { id=item.EmployeeJOBSID }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.EmployeeJOBSID })
        </td>
    </tr>
}

</table>
